    "deploy-local": "graph deploy MooToken --ipfs http://localhost:5001 --node http://127.0.0.1:8020"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
